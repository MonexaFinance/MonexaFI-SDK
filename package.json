{
  "name": "monexa-finance-sdk",
  "version": "1.0.0",
  "description": "Official TypeScript SDK for Monexa Finance - Convert crypto to virtual cards instantly",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs && npm run build:types",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:types": "tsc -p tsconfig.types.json",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "example": "ts-node examples/usage.ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "monexa",
    "finance",
    "crypto",
    "solana",
    "virtual-card",
    "payment",
    "web3",
    "defi",
    "fintech",
    "sdk"
  ],
  "author": "Monexa Finance",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/monexa-finance/sdk"
  },
  "homepage": "https://monexa.finance",
  "bugs": {
    "url": "https://github.com/monexa-finance/sdk/issues"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "ts-node": "^10.9.0"
  },
  "peerDependencies": {
    "@solana/web3.js": "^1.87.0"
  },
  "peerDependenciesMeta": {
    "@solana/web3.js": {
      "optional": true
    }
  }
}
